include includes/header.pug 

.banner 
    -   
        var slides = [
            {
                title: "Women Dress",
                path: "dress4.jpg"
            },
            {
                title: "New Fashion for wondefull women",
                path: "dress5.jpg"
            },
            {
                title: "Catalinas of Georgoes girl!",
                path: "dress6.jpg"
            },
            {
                title: "The trend of the 20th Century",
                path: "dress7.jpg"
            }
        ];
    each slide in slides 
        .slide.slide1 
            img(src="./public/images/" + slide.path, alt="", srcset="")
            .slide-info 
                h2.slide-text= slide.title
.coll-container
    - var items = articles
    .recommended-art
        br
        h2.section-title New Fashion for real women 
        br
        .rec-dress-box 
            each dress in items 
                .dress 
                    .img-box.dress-img 
                        img(src="./public/images/" + dress.imageURL, alt="#{dress.title}")
